{
  "name": "cline-acp",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.3",
  "description": "An ACP-compatible coding agent powered by Cline",
  "main": "dist/cline/index.js",
  "bin": {
    "cline-acp": "./dist/cline-index.js"
  },
  "type": "module",
  "files": [
    "dist/",
    "proto/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cline-index.js",
    "dev": "npm run build && npm run start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "test": "vitest",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cline",
    "acp",
    "agent",
    "typescript",
    "zed"
  ],
  "homepage": "https://github.com/Tonksthebear/cline-acp#readme",
  "bugs": {
    "url": "https://github.com/Tonksthebear/cline-acp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tonksthebear/cline-acp.git"
  },
  "author": "Jason C",
  "license": "Apache-2.0",
  "dependencies": {
    "@agentclientprotocol/sdk": "0.5.1",
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "@vitest/ui": "4.0.13",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "4.0.13"
  }
}
